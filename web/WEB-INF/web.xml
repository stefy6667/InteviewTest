<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>LServlet</servlet-name>
        <servlet-class>Servlet.LServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LServlet</servlet-name>
        <url-pattern>/SearchUser</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ResultServlet</servlet-name>
        <servlet-class>Servlet.ResultServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResultServlet</servlet-name>
        <url-pattern>/Result</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddServlet</servlet-name>
        <servlet-class>Servlet.AddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddServlet</servlet-name>
        <url-pattern>/AddUser</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FizzBuzzServlet</servlet-name>
        <servlet-class>Servlet.FizzBuzzServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FizzBuzzServlet</servlet-name>
        <url-pattern>/FizzBuzz</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DuplicateServlet</servlet-name>
        <servlet-class>Servlet.DuplicateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DuplicateServlet</servlet-name>
        <url-pattern>/Duplicate</url-pattern>
    </servlet-mapping>
</web-app>